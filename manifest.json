{
  "manifest_version": 2,
  "name": "Activiter2",
  "short_name": "Activiter2",
  "description": "Description for Activiter2",

  "version": "0.0.1.0",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
